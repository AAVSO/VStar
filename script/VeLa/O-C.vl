o_minus_c(times:list period:real) : list {
  calc <- []
  oc <- []
  t <- nth(times 0)
  n <- 0
  while n < length(times) {
    calc <- append(calc t)
    delta <- nth(times n)  - t
    oc <- append(oc delta)
    t <- t+period
    n <- n+1
  }
  oc
}

