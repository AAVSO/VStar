# Y combinator

Y is λ(h : function) : function {
    λ(f : function) : function {
        f(f)
    } (λ(f : function) : function {
        h(λ(n : integer) : integer {
            (f(f))(n)
        })
    })
}

result is (Y(λ(g : function) : function {
    λ(n : integer) : integer {
        if n < 2 then 1 else n * g(n - 1)
    }
})) (8)

